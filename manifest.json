{
  "manifest_version": 2,
  "name": "Jike Redirect",
  "version": "1.0",
  "description": "Redirects Jike mobile links to web links.",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "manifest_url": "https://web.okjike.com/*",
  "icons": {
    "48": "images/icon48.png", 
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Jike Redirect"
  }
}
